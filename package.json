{
  "name": "web",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "seed": "blitz run db seed",
    "steward": "blitz run db seed -f db/steward.ts -e production"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.4.2",
    "@heroicons/react": "1.0.5",
    "@prisma/client": "3.6.0",
    "autoprefixer": "10.4.0",
    "aws-sdk": "2.1060.0",
    "blitz": "0.45.2",
    "ethers": "5.5.2",
    "final-form": "4.20.6",
    "formidable-serverless": "1.1.1",
    "graphql": "16.2.0",
    "graphql-request": "3.7.0",
    "jmespath": "0.16.0",
    "keccak": "3.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "11.5.1",
    "react-final-form": "6.5.7",
    "react-select": "5.2.2",
    "tailwindcss": "3.0.1",
    "uuid": "8.3.2",
    "validator": "13.7.0",
    "wagmi": "0.2.6",
    "zod": "3.11.6",
    "zustand": "3.6.8"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.37",
    "eslint": "7.32.0",
    "file-loader": "6.2.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.6.0",
    "pretty-quick": "3.1.2",
    "preview-email": "3.0.5",
    "prisma": "3.6.0",
    "typescript": "~4.5",
    "url-loader": "4.1.1"
  },
  "private": true
}
